<UserControl x:Class="SLPhotoTest.PhotoEdit.PhotoIO"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400" Width="400" Height="35">
    
    <Grid x:Name="LayoutRoot" Background="White">
        <StackPanel Height="35" HorizontalAlignment="Left" Name="stackPanel1" VerticalAlignment="Top" Width="300" Orientation="Horizontal">
            <Button Content="New" Height="23" Name="buttonNew" Width="50" Click="buttonNew_Click" />

            <Button Content="Open" Height="23" Name="buttonOpen" Width="50" Click="buttonOpen_Click" />
            <Button Content="Save" Height="23" Name="buttonSave" Width="50" Click="buttonSave_Click" />
            <Button Content="Copy" Height="23" Name="buttonCopy" Width="50" Click="buttonCopy_Click" />
            <Button Content="Paste" Height="23" Name="buttonPaste" Width="50" Click="buttonPaste_Click" />
            <Button Content="Close" Height="23" Name="buttonClose" Width="50" Click="buttonClose_Click" />
        </StackPanel>
    </Grid>
</UserControl>
