<UserControl x:Class="SLPhotoTest.PhotoEdit.PhotoClipboard"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="120" d:DesignWidth="400">
    
    <Grid x:Name="LayoutRoot" Background="White">
        <ScrollViewer Height="75" HorizontalAlignment="Left" Margin="0,45,0,0" Name="ClipBoardView" VerticalAlignment="Top" Width="400" HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Auto" />
        <Grid Height="45" HorizontalAlignment="Left" Margin="0,0,0,0" Name="grid1" VerticalAlignment="Top" Width="400" >
            <Button Content="Input" Height="30" HorizontalAlignment="Left" Margin="3,5,0,0" Name="buttonInput" VerticalAlignment="Top" Width="49" />
            <Button Content="Delete" Height="29" HorizontalAlignment="Left" Margin="58,6,0,0" Name="buttonDelete" VerticalAlignment="Top" Width="49" />
            <Button Content="Clear" Height="32" HorizontalAlignment="Left" Margin="113,5,0,0" Name="buttonClear" VerticalAlignment="Top" Width="57" />
            <Button Content="X" Height="31" HorizontalAlignment="Left" Margin="362,6,0,0" Name="buttonClose" VerticalAlignment="Top" Width="34" />

        </Grid>
    </Grid>
</UserControl>
