<UserControl x:Class="SilverlightLynxControls.Data.LynxDataRowList"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">
    
    <StackPanel x:Name="LayoutRoot" Background="White">
        <StackPanel x:Name="Col" Background="White" Orientation="Horizontal" Height="30">

        </StackPanel>

        <StackPanel x:Name="RowList" Background="White" Height="247">

        </StackPanel>

        
        <StackPanel x:Name="PageNav" Background="White" Orientation="Horizontal">
            <Button Content="First" Height="23" Name="buttonFirst" Width="39" Click="buttonFirst_Click" />
            <Button Content="Prev" Height="23" Name="buttonPrev" Width="39" Click="buttonPrev_Click" />
            <ComboBox Height="23" Name="comboBoxPage" Width="120" DropDownClosed="comboBoxPage_DropDownClosed" />
            <Button Content="Next" Height="23" Name="buttonNext" Width="39" Click="buttonNext_Click" />
            <Button Content="Last" Height="23" Name="buttonlast" Width="39" Click="buttonlast_Click" />
            <TextBlock Height="23" Name="textBlockInfor" Text="Page Infor" />
        </StackPanel>
    </StackPanel>
</UserControl>
